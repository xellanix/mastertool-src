                <Button Content="Open Result" Click="OpenResultClick"/>
            </muxc:InfoBar.ActionButton>
        </muxc:InfoBar>
        
        <ScrollViewer HorizontalScrollMode="Disabled" Padding="0,0,20,20" Grid.Row="1">
